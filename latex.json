{
  "Placeholder Snippet": {
    "prefix": "desc",
    "body": [
      "\\begin{description}",
      "\t\\item[$1] $0",
      "\\end{description}"
    ],
    "description": "Create a description environment"
  },
  "Figure Environment": {
    "prefix": "fig",
    "body": [
      "\\begin{figure}[ht]",
      "\t\\centering",
      "\t\\includegraphics[width=0.8\\linewidth]{$1}",
      "\t\\caption{${2:Caption}}",
      "\t\\label{fig:${3:label}}",
      "\\end{figure}"
    ],
    "description": "Insert a figure environment"
  },
  "Equation Environment": {
    "prefix": "eq",
    "body": [
      "\\begin{equation}",
      "\t$0",
      "\\end{equation}"
    ],
    "description": "Insert an equation environment"
  },
  "Align Environment": {
    "prefix": "algn",
    "body": [
      "\\begin{align*}",
      "\t$1",
      "\\end{align*}",
      "$0"
    ],
    "description": "Insert an align environment"
  },
  "Inline Math": {
    "prefix": "mm",
    "body": ["$$1$ $0"],
    "description": "Inline math mode $ ... $"
  },
  "Display Math": {
    "prefix": "dm",
    "body": [
      "\\[",
      "\t$1",
      "\\]",
      "$0"
    ],
    "description": "Display math mode \\[ ... \\]"
  }
}
